msgid "Never show again"
msgstr "Больше никогда не показывать"

#: src/daemon/hookevent/hookgui.cpp:54
msgid "Update Information"
msgstr "Сведения об обновлении"

#: src/daemon/hookevent/hookgui.cpp:96
