msgid "Flash"
msgstr "Flash"

#: src/daemon/installevent/installevent.cpp:36
msgid "K3b CD Codecs"
msgstr "K3b CD кодеци"

#: src/daemon/installevent/installevent.cpp:37
msgid "MP3 Encoding"
msgstr "Кодиране в MP3"

#: src/daemon/installevent/installevent.cpp:39
