#: src/daemon/l10nevent/l10nevent.cpp:124
#, kde-format
msgctxt "Installs additional localization packages"
msgid "Install"
msgstr "Instal·la"

#: src/daemon/rebootevent/rebootevent.cpp:55
#, kde-format
